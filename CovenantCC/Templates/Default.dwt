        <div class="grid4column newsletter-signup">
        <h4>Subscribe</h4>
        <form name="catemaillistform37391" onsubmit="return checkWholeForm37391(this)" method="post" action="/CampaignProcess.aspx?ListID=23908">
            <div class="form">
            <div class="item">
            <label for="CLFullName">Full Name</label>
            <br />
            <input type="text" class="cat_textbox_small" name="FullName" id="CLFullName" maxlength="255" />
            </div>
            <div class="item">
            <label for="CLEmailAddress">Email Address</label>
            <br />
            <input type="text" class="cat_textbox_small" name="EmailAddress" id="CLEmailAddress" maxlength="255" />
            </div>
            <div class="item">
            <label>Enter Word Verification in box below</label>
            <br />
            {module_captchav2}</div>
            <div class="item">
            <input type="submit" class="cat_button" value="Subscribe" id="catlistbutton" />
            </div>
            </div>
            <script type="text/javascript" src="/CatalystScripts/ValidationFunctions.js">
					</script>
            <script type="text/javascript">
					//<![CDATA[
					function checkWholeForm37391(theForm){var why = "";if (theForm.EmailAddress) why += checkEmail(theForm.EmailAddress.value);if (theForm.CaptchaV2) why += isEmpty(theForm.CaptchaV2.value, "Enter Word Verification in box below"); if (why != ""){alert(why);return false;}theForm.submit();return false;}
					//]]>
					</script>
        </form>
        </div>
        <div class="grid4column news">
        <h4>LOGIN ZONE</h4>
        <form name="catseczoneform49987" onsubmit="return checkWholeForm49987(this)" method="post" action="/ZoneProcess.aspx?ZoneID=-1&amp;OID={module_oid}&amp;OTYPE={module_otype}">
            <div class="form">
            <div class="item"><label for="SZUsername">Username</label><br />
            <input type="text" class="cat_textbox_small" name="Username" id="SZUsername" maxlength="255" /></div>
            <div class="item"><label for="SZPassword">Password</label><br />
            <input type="password" class="cat_textbox_small" name="Password" id="SZPassword" maxlength="255" /></div>
            <div class="item"><input type="checkbox" name="RememberMe" id="RememberMe" /><label for="RememberMe">Remember Me</label></div>
            <div class="item"><input type="submit" class="cat_button" value="Log in" /><a href="/_System/SystemPages/PasswordRetrieveRequest">Lost password?</a></div>
            </div>
            <script type="text/javascript" src="/CatalystScripts/ValidationFunctions.js"></script>
            <script type="text/javascript">
//<![CDATA[
function checkWholeForm49987(theForm){var why = "";if (theForm.Username) why += isEmpty(theForm.Username.value, "Username");if (theForm.Password) why += isEmpty(theForm.Password.value, "Password");if (why != ""){alert(why);return false;}theForm.submit();return false;}
//]]>
</script>
        </form>
